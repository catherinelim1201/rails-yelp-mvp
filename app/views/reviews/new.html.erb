<div class="container">

  <h1>New review</h1>
    <%= simple_form_for [@restaurant, @review] do |f| %>
      <%= f.input :content %>
      <%= f.input :rating %>
      <%= f.submit "Submit review", class: "btn btn-primary" %>
    <% end %>

    <button class="btn">
      <%= link_to "Back to restaurant", restaurant_path(@restaurant) %>
    </button>



</div>
